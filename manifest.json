{
    "manifest_version": 3,
    "name": "Clickable Images",
    "version": "1.1",
    "description": "Make all images clickable by wrapping them with <a> tag.",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "src/content.js"
            ]
        }
    ],
    "icons": {
        "32": "images/icon32.png",
        "72": "images/icon72.png",
        "128": "images/icon128.png"
    }
}